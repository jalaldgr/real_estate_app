                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:backgroundTint="@color/colorPrimary"
                    android:entries="@array/new_land_water_electricity" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:paddingTop="4dp"
            android:paddingBottom="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:layout_weight="0.5"
                android:background="@drawable/border_linear_layout_new_land"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/land_info_province"
                    android:textColor="@color/textview_lable"
                    android:textSize="@dimen/smal_text_size" />

                <Spinner
                    android:id="@+id/NewLandRentFragmentProvinceSpnr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:backgroundTint="@color/colorPrimary" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:layout_weight="0.5"
                android:background="@drawable/border_linear_layout_new_land"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/land_info_city"
                    android:textColor="@color/textview_lable"
                    android:textSize="@dimen/smal_text_size" />

                <Spinner
                    android:id="@+id/NewLandRentFragmentCitySpnr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:backgroundTint="@color/colorPrimary" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:paddingTop="4dp"
            android:paddingBottom="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:layout_weight="0.5"
                android:background="@drawable/border_linear_layout_new_land"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/new_land_area"
                    android:textColor="@color/textview_lable"
                    android:textSize="@dimen/smal_text_size" />

                <Spinner
                    android:id="@+id/NewLandRentFragmentAreaSpnr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:backgroundTint="@color/colorPrimary" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:layout_weight="0.5"
                android:background="@drawable/border_linear_layout_new_land"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/vocabulary_district"
                    android:textColor="@color/textview_lable"
                    android:textSize="@dimen/smal_text_size" />

                <Spinner
                    android:id="@+id/NewLandRentFragmentDistrictSpnr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:backgroundTint="@color/colorPrimary" />
            </LinearLayout>

        </LinearLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/NewLandRentFragmentAddressLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:hint="آدرس">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/NewLandRentFragmentAddressTxt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/NewLandRentFragmentDescriptionLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:hint="توضیحات">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/NewLandRentFragmentDescriptionTxt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </com.google.android.material.textfield.TextInputLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="240dp">

            <com.google.android.gms.maps.MapView
                android:id="@+id/mapView"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" />

            <Button
                android:id="@+id/NewLandRentFragmentAddMapBtn"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|center_horizontal"
                android:text="@string/select_map"
                android:theme="@style/Widget.MaterialComponents.Button.OutlinedButton" />
        </FrameLayout>

        <EditText
            android:id="@+id/NewLandRentFragmentOnMapTxt"
            android:layout_width="wrap_content"
            android:enabled="false"
            android:layout_height="wrap_content" />


        <Button
            android:id="@+id/NewLandRentFragmentSubmitBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="8dp"
            android:text="@string/new_land_submit"
            android:theme="@style/Widget.MaterialComponents.Button.OutlinedButton" />
    </LinearLayout>
</androidx.core.widget.NestedScrollView>

